<div id="<%= dom_id user_echange %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= user_echange.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= user_echange.content %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Wiki:</strong>
    <%= user_echange.wiki %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= user_echange.status %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Slug:</strong>
    <%= user_echange.slug %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Course:</strong>
    <%= user_echange.course_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= user_echange.user_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this user echange", user_echange, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this user echange', edit_user_echange_path(user_echange), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
